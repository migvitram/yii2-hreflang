{
    "name": "migvitram/yii2-hreflang",
    "description": "Yii 2 extension for hreflang tags generating",
    "type": "yii2-extension",
    "minimum-stability": "dev",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mihail Korchemliuk",
            "email": "migvitram@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "migvitram\\yii2-hreflang\\": "src"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://path.to/your/repo"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
